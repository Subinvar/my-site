{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": [
    "src/app/**/*.{ts,tsx}",
    "src/lib/**/*.ts"
  ],
  "project": [
    "src/**/*.{ts,tsx}"
  ],
  "ignoreDependencies": [
    "lightningcss-win32-x64-msvc",
    "sharp",
    "tailwindcss",
